---
import OfferingContainer from "@/components/shared/OfferingContainer.astro";
import Layout from "@/layouts/Layout.astro";
import Offering from "@/components/Offering.astro";
import offerings from "../../data/dataQuenas";
import TitlePage from "@/components/TitlePage.astro";
---

<Layout
	title="Catálogo"
	description="Este es mi Catálogo de quenas profesionales en bambú, ébano y maderas de lujo"
	image="/MainSiteImage.jpg">
	<TitlePage
		title="Catálogo"
		description="Este es mi Catálogo de instrumentos musicales. En cada tarjeta podrás ver su precio en peso chileno y en dólar americano, un video demostrativo desde mi Canal de YouTube y contactarme para comprar"
	/>
	<OfferingContainer>
		{offerings.map((item) => <Offering {...item} />)}
	</OfferingContainer>
</Layout>
